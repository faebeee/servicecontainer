{
  "opts": {
    "encoding": "utf8",               // same as -e utf8
    "destination": "./docs",          // same as -d ./out/
    "recurse": true                  // same as -r
  },
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc"]
  },
  "source": {
    "include" : ["./src/"]
  },
  "plugins": [],
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  }
}